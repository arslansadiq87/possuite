<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="IconBrush.Primary" Color="{Binding Source={StaticResource Clr.Primary}, Path=Color}"/>

	<!-- Plus / Add icon -->
	<DrawingImage x:Key="Icon.Add">
		<DrawingImage.Drawing>
			<GeometryDrawing Brush="{StaticResource Clr.Primary}">
				<GeometryDrawing.Geometry>
					<PathGeometry Figures="M31,15 H21 V5 
                                 A3,3 0 1 0 15,5 V15 H5 
                                 A3,3 0 1 0 5,21 H15 V31 
                                 A3,3 0 1 0 21,31 V21 H31 
                                 A3,3 0 1 0 31,15 Z"/>
				</GeometryDrawing.Geometry>
			</GeometryDrawing>
		</DrawingImage.Drawing>
	</DrawingImage>

	<!-- Minus / Remove icon -->
	<DrawingImage x:Key="Icon.Remove">
		<DrawingImage.Drawing>
			<GeometryDrawing Brush="{StaticResource Clr.Primary}">
				<GeometryDrawing.Geometry>
					<PathGeometry Figures="M34,18 
                                 a3,3 0 0 1 -3,3 
                                 H5 
                                 a3,3 0 1 1 0,-6 
                                 h26 
                                 a3,3 0 0 1 3,3 z"/>
				</GeometryDrawing.Geometry>
			</GeometryDrawing>
		</DrawingImage.Drawing>
	</DrawingImage>

	<!-- Close / Cancel (X) icon -->
	<DrawingImage x:Key="Icon.Close">
		<DrawingImage.Drawing>
			<GeometryDrawing Brush="{StaticResource Clr.Primary}">
				<GeometryDrawing.Geometry>
					<PathGeometry Figures="M11.383,13.644
                                 A1.03,1.03 0 0 1 9.928,15.1
                                 L6,11.172
                                 L2.072,15.1
                                 A1.03,1.03 0 1 1 0.617,13.644
                                 L4.545,9.716
                                 L0.617,5.79
