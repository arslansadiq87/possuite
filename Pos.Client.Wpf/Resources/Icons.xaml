	<!-- Warehouse / Building icon -->
	<DrawingImage x:Key="Icon.Warehouse">
		<DrawingImage.Drawing>
			<DrawingGroup>

				<!-- Roof + main structure -->
				<GeometryDrawing Brush="#444">
					<GeometryDrawing.Geometry>
						<PathGeometry Figures="M16,4 L7.94,0 L0,4 V5 H1 V16 H3 V7 H13 V16 H15 V5 H16 V4 Z
                                   M4,6 V5 H6 V6 H4 Z
                                   M7,6 V5 H9 V6 H7 Z
                                   M10,6 V5 H12 V6 H10 Z"/>
					</GeometryDrawing.Geometry>
				</GeometryDrawing>

				<!-- Left door -->
				<GeometryDrawing Brush="#444">
					<GeometryDrawing.Geometry>
						<PathGeometry Figures="M6,9 H5 V8 H4 V11 H7 V8 H6 V9 Z"/>
					</GeometryDrawing.Geometry>
				</GeometryDrawing>

				<!-- Middle door -->
				<GeometryDrawing Brush="#444">
					<GeometryDrawing.Geometry>
						<PathGeometry Figures="M6,13 H5 V12 H4 V15 H7 V12 H6 V13 Z"/>
					</GeometryDrawing.Geometry>
				</GeometryDrawing>

				<!-- Right door -->
				<GeometryDrawing Brush="#444">
					<GeometryDrawing.Geometry>
						<PathGeometry Figures="M10,13 H9 V12 H8 V15 H11 V12 H10 V13 Z"/>
					</GeometryDrawing.Geometry>
				</GeometryDrawing>

			</DrawingGroup>
		</DrawingImage.Drawing>
	</DrawingImage>

	<DrawingImage x:Key="Icon.Product">
		<DrawingImage.Drawing>
			<DrawingGroup>
				<!-- Outer hexagon -->
				<GeometryDrawing>
					<GeometryDrawing.Pen>
						<Pen Brush="#212121" Thickness="1.5" LineJoin="Round"/>
					</GeometryDrawing.Pen>
					<GeometryDrawing.Geometry>
						<PathGeometry Figures="M22,7 L12,2 L2,7 L2,17 L12,22 L22,17 Z"/>
					</GeometryDrawing.Geometry>
				</GeometryDrawing>

				<!-- Left diagonal -->
				<GeometryDrawing>
					<GeometryDrawing.Pen>
						<Pen Brush="#212121" Thickness="1.5" StartLineCap="Round" EndLineCap="Round"/>
					</GeometryDrawing.Pen>
					<GeometryDrawing.Geometry>
						<LineGeometry StartPoint="2,7" EndPoint="12,12"/>
					</GeometryDrawing.Geometry>
				</GeometryDrawing>

				<!-- Vertical center -->
				<GeometryDrawing>
					<GeometryDrawing.Pen>
						<Pen Brush="#212121" Thickness="1.5" StartLineCap="Round" EndLineCap="Round"/>
					</GeometryDrawing.Pen>
